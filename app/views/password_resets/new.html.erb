<% provide(:title, 'Email Password') %>
<div class="jonestitle"> <h1>Email Password</h1> </div>
<div class="container">
<div class="row">
<%= form_tag password_resets_path, :method => :post do %>
  <div class="col-xs-10 col-xs-offset-1 col-sm-8 col-sm-offset-2 col-md-6 col-md-offset-3 ">
    <%= label_tag :email %><br>
    <%= text_field_tag :email, params[:email], class: "form-control input-md" %>
 <div><%= button_tag "Send Email", class: "btn btn-lg btn-primary" %></div>
  </div>

<% end %>

</div>

</div>


